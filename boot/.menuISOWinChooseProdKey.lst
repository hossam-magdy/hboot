# This menu is for choosing which PRODUCT KEY should be used in windows 
# installation based on which EDITION desired to be installed
# %MYISO% variable should be already set

#if exist (hd0,1)/boot/.swap_partitions.g4b (hd0,1)/boot/.swap_partitions.g4b (hd0) 1 SURE

timeout 30
default 0
if exist (hd0,1)/boot/.swap_partitions.g4b (hd0,1)/boot/.swap_partitions.g4b (hd0) 1 SURE
find --set-root /menu.lst


title << Main Menu
configfile ()/menu.lst


title Install ( %MYISO% ) using product key:
root()


title NPPR9-FWDCX-D2C8J-H872K-2YT43: Windows 10 Enterprise
set KEYP=NPPR9-FWDCX-D2C8J-H872K-2YT43
cat ()/boot/firadisk/auWin.xml > ()/autounattend.xml
cat --locate=W8T8K --replace=%KEYP% ()/autounattend.xml > nul
cat ()/boot/spaces.txt > ()/boot/firadisk/ISONAME.CMD
write ()/boot/firadisk/ISONAME.CMD SET MYISO=\\%MYISO%\r\n
map /%MYISO% (0xff)
map (hd0) (hd1)
map (hd1) (hd0)
map --hook
chainloader (0xff)/BOOTMGR || chainloader (0xff)
boot


